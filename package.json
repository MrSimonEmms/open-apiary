{
  "name": "open-apiary",
  "version": "0.0.0",
  "description": "Apiary management software",
  "author": "Simon Emms",
  "private": true,
  "license": "MIT",
  "main": "./server/main",
  "scripts": {
    "audit": "node node_modules/better-npm-audit audit -i 1488",
    "prebuild": "rimraf ./dist ./.nuxt",
    "build": "run-s compile:server compile:client",
    "postbuild": "run-s copy:nuxt copy:client copy:config copy:dist",
    "compile:client": "cross-env mode=production nuxt build",
    "compile:server": "tsc -p ./server/tsconfig.build.json",
    "copy:client": "copyfiles -a \"client/**/*\" dist",
    "copy:dist": "copyfiles ATTRIBUTION.md CHANGELOG LICENSE pm2.yaml README.md dist",
    "copy:config": "copyfiles nuxt.config.js package.json package-lock.json dist && mkdirp ./dist/log && node ./scripts/editPackageJson.js ./dist/package.json",
    "copy:nuxt": "copyfiles -a \".nuxt/**/*\" dist",
    "dev": "cross-env IS_NUXT_ENABLED=true nodemon | pino-pretty -tlc",
    "dev:sqlite": "rm -f ./db.sql && concurrently  'npm run -s dev:sqlite:run' 'npm run -s dev:sqlite:db'",
    "dev:sqlite:db": "cd ./data && cross-env UPLOAD_PATH=$PWD/../uploads cross-env DB_TYPE=sqlite cross-env DB_NAME=../db.sql cross-env MAX_TRIES=15 npm start",
    "dev:sqlite:run": "cross-env LOG_DESTINATION=STDOUT cross-env LOG_LEVEL=info cross-env UPLOAD_PATH=$PWD/uploads cross-env DB_TYPE=sqlite cross-env DB_NAME=./db.sql cross-env JWT_SECRET=jwt-secret-token cross-env DB_MIGRATIONS_RUN=true cross-env DB_SYNC=false cross-env DB_LOGGING=true npm run dev",
    "dev:client": "nuxt",
    "dev:server": "nodemon",
    "docs:dev": "run-s docs:pre && nodemon -x 'vuepress dev docs --debug --no-cache'",
    "docs:build": "run-s docs:pre && vuepress build docs --dest public",
    "docs:pre": "mkdir -p ./docs/contributing && cp ./CONTRIBUTING.md ./docs/contributing/index.md",
    "lint": "run-s lint:client lint:server",
    "lint:client": "eslint --ext .ts,.js,.vue ./client nuxt.config.js",
    "lint:server": "eslint --ext .ts,.js ./server",
    "postinstall": "patch-package",
    "start": "node ./",
    "test": "run-s test:server",
    "test:server": "jest",
    "test:server:watch": "jest --watch",
    "test:server:cov": "jest --coverage",
    "test:server:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:server:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node ./scripts/typeorm"
  },
  "dependencies": {
    "@alienfast/i18next-loader": "^1.1.4",
    "@godaddy/terminus": "^4.3.1",
    "@nestjs/common": "^6.7.2",
    "@nestjs/config": "0.0.9",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^4.3.2",
    "@nestjs/terminus": "^6.5.6",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjsx/crud": "^4.4.1",
    "@nestjsx/crud-typeorm": "^4.4.1",
    "@nuxtjs/axios": "^5.9.2",
    "@panter/vue-i18next": "^0.15.1",
    "axios": "^0.19.2",
    "better-queue": "^3.8.10",
    "better-queue-memory": "^1.0.4",
    "cache-manager": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cookie-universal": "^2.1.0",
    "filesize.js": "^2.0.0",
    "i18next": "^19.0.3",
    "i18next-express-middleware": "^1.9.1",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^1.5.3",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "luxon": "^1.21.3",
    "marked": "^0.8.0",
    "ms": "^2.1.2",
    "mysql2": "^2.1.0",
    "nestjs-pino": "^1.1.3",
    "nuxt": "^2.0.0",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.0",
    "passport-jwt": "^4.0.0",
    "pino": "^5.16.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "uuid": "^3.3.3",
